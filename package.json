{
  "name": "nexter",
  "version": "1.0.0",
  "description": "Nexter webpage",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch sass/main.scss css/style.css",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 40 versions\" css/style.comp.css -o css/style.prefix.css",
    "compress:css": "sass css/style.prefix.css css/style.css --style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "bugs": {
    "url": "https://github.com/Satx98/project-nexter/issues"
  },
  "homepage": "https://github.com/Satx98/project-nexter#readme",
  "author": "Satadru",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.8.0",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "sass": "^1.26.8"
  }
}
